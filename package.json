{
  "name": "qrcodejs",
  "version": "1.0.0",
  "description": "QRCode Generator",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "compile:typescript": "tsc",
    "test": "mocha --recursive",
    "asbuild:untouched": "asc assembly/index.ts --target debug",
