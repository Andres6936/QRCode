    "QR",
    "QRCode"
  ],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/davidshimjs/qrcodejs/issues"
  },
  "homepage": "https://github.com/davidshimjs/qrcodejs#readme",
  "dependencies": {
    "@assemblyscript/loader": "^0.19.20"
  },
  "devDependencies": {
    "assemblyscript": "^0.19.20",
    "mocha": "^9.1.3",
    "typescript": "^4.5.2"
  }
}
